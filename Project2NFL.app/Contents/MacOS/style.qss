QScrollBar:horizontal {
        max-height: 20px;
        background: #666;
        border: 1px transparent grey;
        margin: 0px 20px 0px 20px;
}
QScrollBar::handle:horizontal {
        background-color: white;
        border-style: solid;
        border-width: 1px;
        border-color: #666;
        min-width: 25px;
}
QScrollBar::handle:horizontal:hover {
        background: #666;
        border-style: solid;
        border-width: 1px;
        border-color: #666;
        min-width: 25px;
}
QScrollBar::add-line:horizontal {
        border: 1px solid;
        border-color: #666;
        background-color:white;
        width: 20px;
        subcontrol-position: right;
        subcontrol-origin: margin;
}
QScrollBar::add-line:horizontal:hover {
        border: 1px solid;
        border-color: #666;

        background-color: white;
        height: 16px;
        width: 16px;
        subcontrol-position: right;
        subcontrol-origin: margin;
}
QScrollBar::add-line:horizontal:pressed {
        border: 1px solid;
        border-color: #666;

        background-color: white;
        height: 16px;
        width: 16px;
        subcontrol-position: right;
        subcontrol-origin: margin;
}
QScrollBar::sub-line:horizontal {
        border: 1px solid;
        border-color: #666;
        background-color:white;
        width: 20px;
        subcontrol-position: left;
        subcontrol-origin: margin;
}
QScrollBar::sub-line:horizontal:hover {
        border: 1px solid;
        border-color: #666;
        border-radius: 8px;
        background-color: white;
        height: 16px;
        width: 16px;
        subcontrol-position: left;
        subcontrol-origin: margin;
}
QScrollBar::sub-line:horizontal:pressed {
        border: 1px solid;
        border-color: grey;

        background-color: white;
        height: 16px;
        width: 16px;
        subcontrol-position: left;
        subcontrol-origin: margin;
}
QScrollBar::left-arrow:horizontal {
        border: 1px transparent grey;

        width: 6px;
        height: 6px;
        background: #666;
}
QScrollBar::right-arrow:horizontal {
        border: 1px transparent grey;

        width: 6px;
        height: 6px;
        background: #666;
}
QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
        background: none;
}
QScrollBar:vertical {
